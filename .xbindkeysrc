# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +2000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -2000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

# Increase Brightness
"changeBacklight --inc 5"
	XF86MonBrightnessUp

# Decrease Brightness
"changeBacklight --dec 5"
	XF86MonBrightnessDown

#Capture full screen
"gscreenshot -s -c"
   Print

"gscreenshot -c"
   Control+Print

"gscreenshot -c -f ~/Pictures/screenshot/"
   Alt+Print

"toggleBacklight"
   Scroll_Lock

"st ranger"
   F5