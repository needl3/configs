#############################
# Dunst Volume Notification #
#############################

# Increase volume
".config/scripts/volume.sh 1"
   XF86AudioRaiseVolume

# Decrease volume
".config/scripts/volume.sh 2"
   XF86AudioLowerVolume

# Mute volume
".config/scripts/volume.sh 3"
   XF86AudioMute

# Mute microphone
".config/scripts/volume.sh 4"
   XF86AudioMicMute

######################
# Brightness Control #
######################

# Increase Brightness
"changeBacklight --inc 5"
	XF86MonBrightnessUp

# Decrease Brightness
"changeBacklight --dec 5"
	XF86MonBrightnessDown

######################
# Screenshot Control #
######################

"flameshot gui"
   Print

"flameshot screen -c"
   Control+Print

"flameshot screen -p ~/Pictures"
   Alt+Print

"toggleBacklight"
   Scroll_Lock

"st ranger"
   F5

"st cmus"
   Control+M

"st vis"
   Control+M

"qterminal"
   Control+Shift+Return